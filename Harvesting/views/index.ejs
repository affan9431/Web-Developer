    <script>
       // Chatbot icon click event
const chatbotIcon = document.getElementById('chatbot-icon');
const chatbotContainer = document.getElementById('chatbot-container');
const chatbotInput = document.getElementById('chatbot-input');

chatbotIcon.addEventListener('click', () => {
    chatbotContainer.style.display = 'block';
});

window.addEventListener('click',()=>{
  if(event.target == chatbotContainer){
    chatbotContainer.style.display='none'
    }
})
// Function to add a chat message
function addMessage(sender, message) {
    const chatbotMessages = document.getElementById('chatbot-messages');
    const messageDiv = document.createElement('div');
    messageDiv.textContent = `[${sender}] ${message}`;
    chatbotMessages.appendChild(messageDiv);
}

// Function to handle user input
chatbotInput.addEventListener('keydown', (e) => {
    if (e.key === 'Enter') {
        const userMessage = chatbotInput.value;
        addMessage('User', userMessage);
        chatbotInput.value = '';

        // Simulate bot response based on user input
        setTimeout(() => {
            let botResponse = '';

            // Implement additional responses based on user input
            if (userMessage.toLowerCase().includes('hii')) {
                botResponse = 'Hello there!';
            } else if (userMessage.toLowerCase().includes('how are you')) {
                botResponse = 'I am just a simple chatbot.';
            } else if (userMessage.toLowerCase().includes('i am facing some problem')) {
                botResponse = 'Please explain your problem.';
            } else {
                botResponse = 'Im not sure how to respond to that.';
            }

            addMessage('Bot', botResponse);
        }, 1000);
    }
});

    </script>
